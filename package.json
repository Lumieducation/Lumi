{
    "name": "lumi",
    "version": "0.3.1",
    "description": "A tool to create and display interactive content with H5P",
    "author": "J.P. Schellenberg",
    "main": "build/main.js",
    "dependencies": {
        "@sentry/electron": "2.0.4",
        "@sentry/node": "5.29.2",
        "body-parser": "1.19.0",
        "debug": "4.3.1",
        "electron-log": "4.3.1",
        "electron-updater": "4.3.5",
        "express": "4.17.1",
        "express-fileupload": "1.2.0",
        "fs-extra": "9.0.1",
        "h5p-nodejs-library": "6.2.0",
        "nucleus-nodejs": "3.0.7",
        "mkdirp": "1.0.4",
        "promisepipe": "3.0.0",
        "rimraf": "3.0.2",
        "socket.io": "2.3.0"
    },
    "scripts": {
        "build": "npm run build:client && npm run build:server",
        "build:all": "npm run build && ./node_modules/.bin/electron-builder --config builder.config.js --mac --win --linux --publish never",
        "build:all:dev": "CSC_IDENTITY_AUTO_DISCOVERY=false npm run build:all",
        "build:client": "node scripts/build.js",
        "build:mac": "npm run build && ./node_modules/.bin/electron-builder --config builder.config.js --mac --publish never",
        "build:mac:dev": "CSC_IDENTITY_AUTO_DISCOVERY=false npm run build:mac",
        "build:sentry": "RELEASE=true npm run build:client",
        "build:server": "npx tsc --project tsconfig.server.json",
        "build:win": "npm run build && cp -r electron/assets/appx build && npx --no-install electron-builder --config builder.config.js --win --publish never && rm -rf build/appx",
        "ci": "npm run lint && npm run format:check && npm run build && npm run test",
        "clean": "rm -rf build/",
        "format": "npx prettier --write \"{src,test}/**/*.ts\"",
        "format:check": "npx prettier --check \"{src,test}/**/*.ts\"",
        "lint": "./node_modules/.bin/tslint --project tsconfig.json --config tslint.json",
        "update:api": "node scripts/api.js && npm run format",
        "publish:mac": "npx --no-install electron-builder --config builder.config.js --mac --publish always",
        "publish:win": "cp -r electron/assets/appx build && npx --no-install electron-builder --config builder.config.js --win --publish always && rm -rf build/appx",
        "publish:linux": "npx --no-install electron-builder --config builder.config.js --linux --publish always",
        "postinstall": "sh scripts/install.sh 1.24 && sh scripts/install.sh 1.24.1",
        "start": "node scripts/start.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:mac": "npx jest --config jest.mac.config.js",
        "test:e2e:win": "npx jest --config ./test/e2e/win/jest.config.js",
        "semantic-release": "semantic-release",
        "uninstall": "rm -rf build/ && rm -rf node_modules/"
    },
    "publishConfig": {
        "access": "restricted"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm run format:check",
            "pre-push": "npm run ci",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "7.12.10",
        "@commitlint/cli": "11.0.0",
        "@commitlint/config-conventional": "11.0.0",
        "@fortawesome/fontawesome-svg-core": "1.2.32",
        "@fortawesome/free-brands-svg-icons": "5.15.1",
        "@fortawesome/free-solid-svg-icons": "5.15.1",
        "@fortawesome/react-fontawesome": "0.1.14",
        "@hot-loader/react-dom": "16.14.0",
        "@material-ui/core": "4.11.2",
        "@material-ui/icons": "4.11.2",
        "@material-ui/lab": "4.0.0-alpha.57",
        "@material-ui/styles": "4.11.2",
        "@semantic-release/git": "9.0.0",
        "@sentry/browser": "5.29.2",
        "@sentry/types": "5.29.2",
        "@sentry/webpack-plugin": "1.14.0",
        "@svgr/webpack": "5.5.0",
        "@types/body-parser": "1.19.0",
        "@types/debug": "4.1.5",
        "@types/electron": "1.6.10",
        "@types/express": "4.17.9",
        "@types/history": "4.7.8",
        "@types/jest": "26.0.19",
        "@types/lodash": "4.14.167",
        "@types/moment": "2.13.0",
        "@types/node": "12.19.11",
        "@types/react": "16.14.2",
        "@types/react-dom": "16.9.10",
        "@types/react-redux": "7.1.15",
        "@types/react-router-dom": "5.1.7",
        "@types/redux-logger": "3.0.8",
        "@types/redux-mock-store": "1.0.2",
        "@types/redux-sentry-middleware": "0.1.2",
        "@types/shortid": "0.0.29",
        "@types/socket.io": "2.1.12",
        "@types/socket.io-client": "1.4.34",
        "@types/string-hash": "1.1.1",
        "@types/superagent": "4.1.10",
        "@types/supertest": "2.0.10",
        "@typescript-eslint/eslint-plugin": "4.11.1",
        "@typescript-eslint/parser": "4.11.1",
        "babel-eslint": "10.1.0",
        "babel-jest": "26.6.3",
        "babel-loader": "8.2.2",
        "babel-plugin-named-asset-import": "0.3.7",
        "babel-preset-react-app": "9.1.2",
        "camelcase": "6.2.0",
        "case-sensitive-paths-webpack-plugin": "2.3.0",
        "clsx": "1.1.1",
        "connected-react-router": "6.8.0",
        "css-loader": "5.0.1",
        "dotenv": "8.2.0",
        "dotenv-expand": "5.1.0",
        "electron": "10.2.0",
        "electron-builder": "22.9.1",
        "electron-notarize": "1.0.0",
        "eslint": "7.17.0",
        "eslint-config-react-app": "5.2.1",
        "eslint-loader": "4.0.2",
        "eslint-plugin-flowtype": "5.2.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-react": "7.22.0",
        "eslint-plugin-react-hooks": "4.2.0",
        "file-loader": "6.2.0",
        "history": "4.10.1",
        "html-webpack-plugin": "4.5.0",
        "husky": "4.3.6",
        "identity-obj-proxy": "3.0.0",
        "is-wsl": "2.2.0",
        "jest": "26.6.3",
        "jest-environment-jsdom-fourteen": "1.0.1",
        "jest-watch-typeahead": "0.6.1",
        "lodash": "4.17.20",
        "mini-css-extract-plugin": "1.3.3",
        "moment": "2.29.1",
        "notistack": "1.0.3",
        "optimize-css-assets-webpack-plugin": "5.0.4",
        "pnp-webpack-plugin": "1.6.4",
        "postcss-flexbugs-fixes": "5.0.2",
        "postcss-loader": "4.1.0",
        "postcss-normalize": "9.0.0",
        "postcss-preset-env": "6.7.0",
        "postcss-safe-parser": "5.0.2",
        "prettier": "2.2.1",

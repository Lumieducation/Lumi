        "react-app-polyfill": "1.0.6",
        "react-dev-utils": "10.2.1",
        "react-dom": "16.14.0",
        "react-hot-loader": "4.13.0",
        "react-redux": "7.2.2",
        "react-router": "5.2.0",
        "react-router-dom": "5.2.0",
        "redux": "4.0.5",
        "redux-logger": "3.0.6",
        "redux-mock-store": "1.5.4",
        "redux-sentry-middleware": "0.2.2",
        "redux-thunk": "2.3.0",
        "resolve": "1.19.0",

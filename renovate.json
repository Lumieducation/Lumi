{
  "extends": ["config:base"],
  "baseBranches": ["development"],
  "labels": ["[type] chore", "dependencies"],
  "automerge": true,
  "major": {
    "automerge": false
  },
  "lockFileMaintenance": {
    "enabled": true,
    "schedule": "every weekday"
  },
  "prConcurrentLimit": 1,
  "prHourlyLimit": 50,
  "semanticCommits": true,
  "semanticCommitType": "chore"
}

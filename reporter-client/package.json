            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/jest": "28.1.0",
        "@types/node": "17.0.39",
        "@types/react": "18.0.11",
        "@types/react-dom": "18.0.5",
        "mini-css-extract-plugin": "2.6.0",
        "prettier": "2.6.2",
        "react-app-rewired": "^2.2.1",
        "react-scripts": "5.0.1",
        "typescript": "4.7.3"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=7.0.0"
    }
}
